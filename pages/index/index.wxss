.head{
    display: flex;
    align-items: center;
    justify-content: center;
    padding-top: 130rpx;
}
.userlive{
    display: flex;
    align-items: center
}
.userlive text{
    height: 20rpx;
    line-height: 20rpx;
    font-size: 20rpx;
    color: #333333;
}
.userlive image{
    height: 28rpx;
    width: 18rpx;
    padding-left: 4rpx;
    padding-right: 14rpx;
}
.search{
    display: flex;
    width: 576rpx;
    justify-content: center;
    align-items: center;
    border: 1rpx solid #F68747;
    box-sizing: border-box;
    padding-left: 10rpx;
}
.search input{
    flex: 1;
    font-size: 32rpx;
}
.search_btn{
    display: flex;
    align-items: center;
    font-size: 32rpx;
    color: #fff;
    background: #F68747;
    box-sizing: border-box;
    padding: 10rpx ;
}
/*  */

.banner{
    width: 100%;
    height: 312rpx;
    padding-top: 16rpx;
}
.banner image{
    width: 100%;
}
/*  */

.hotel{
    padding: 20rpx 40rpx;
    box-sizing: border-box;
}
.hotel_title{
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 28rpx;
    color: #333;
    font-weight: 300;
}